    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
            "console": "externalTerminal",
            "justMyCode": true,
            "cwd": "${workspaceRoot}",
            "env": {"PYTHONPATH": "${workspaceRoot}"}
        },
        
    ]
}